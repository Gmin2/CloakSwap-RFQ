{
  "name": "cloakswap-maker",
  "version": "1.0.0",
  "description": "Market maker bot for CloakSwap RFQ protocol",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "node --no-warnings --loader ts-node/esm src/index.ts",
    "maker:monitor": "node --no-warnings --loader ts-node/esm src/index.ts monitor",
    "maker:quote": "node --no-warnings --loader ts-node/esm src/index.ts quote",
    "maker:quote-addr": "node --no-warnings --loader ts-node/esm src/index.ts quote-addr",
    "maker:status": "node --no-warnings --loader ts-node/esm src/index.ts status",
    "maker:price": "node --no-warnings --loader ts-node/esm src/index.ts price",
    "maker:tokens": "node --no-warnings --loader ts-node/esm src/index.ts tokens"
  },
  "dependencies": {
    "viem": "^2.21.32",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "@types/node": "^24.3.0"
  }
}